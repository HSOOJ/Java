package triangle;

public class _5_2 {

	public static void main(String[] args) {
		int count = 1;
		
		for(int i = 0; i <3; i++){
			//for(int j = 0; j < i + 1; j++){
			for(int k = 3; k > 3 - i ; k--){
				//System.out.print(count + " ");
				System.out.print( count+k );
				System.out.println(k);
				count ++;
			}
			}System.out.print("\n");
			//}
			}
	}


