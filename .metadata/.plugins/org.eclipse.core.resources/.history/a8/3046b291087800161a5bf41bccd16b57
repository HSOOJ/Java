package triangle;

public class _5_3 {

	public static void main(String[] args) {
		int count = 1;
		int temp = 0;
		
		for (int i = 0; i < 4; i ++){
			for (int k = 3; k > 3 - i; k --){
				for(int j = 0; j < 3; j ++){
					temp += 3 - i;
					System.out.print(count);
				}
				
			}count++;
			System.out.print("\n");
		}

	}

}
