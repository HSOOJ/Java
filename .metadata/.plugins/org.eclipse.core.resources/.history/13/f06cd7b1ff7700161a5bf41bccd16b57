package triangle;

public class _5_4 {

	public static void main(String[] args) {
		int count = 1;
		for(int i = 0; i < 3; i ++){
			for(int k = 3; k  > 3 - i; k--){
				int temp = count + k;
				
				//System.out.print(count + " ");
				System.out.print(temp);
			}System.out.println( );
			count ++;
		}		}

	}

}
