package triangle;

public class _5 {

	public static void main(String[] args) {
		int count = 1;
		
		for(int i = 0; i < 3; i ++){
			for(int j = 0; j < i + 1; j++){
				System.out.print(count + " ");
				int temp = 0;
				for(int k = 3; k  > 3 - i; k--){
					temp += 3 - k;
										
					//System.out.print(count + " ");
					System.out.print(temp);
				}System.out.println( );
				count ++;
			}
			
					}
		
		}
	}


